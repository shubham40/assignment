        "import numpy as np\n",
        "def barabar(low,up,upto):\n",
        "    array = []\n",
        "    step = (up - low) / float(upto-1)\n",
        "    for i in range(upto):\n",
        "        array.append(low)\n",
        "        low = low + step\n",
        "    return array\n",
        "a=barabar(1,10,3) \n",
        "a\n",
        "\n"
